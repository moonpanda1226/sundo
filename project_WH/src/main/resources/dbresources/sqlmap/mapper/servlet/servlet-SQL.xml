<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="servlet">

	<select id="serVletTest" resultType="egovMap">
		<![CDATA[
			SELECT 
				FILENAME, FILEREALNAME
			FROM
				UPLOAD
			WHERE
				1=1
		]]>
	</select>
	
	<select id="suhyun" resultType="servlet.dto.ServletDTO">
		SELECT sd_nm, sd_cd FROM tl_sd
	</select>
	
	<select id="zzang9" parameterType="String" resultType="servlet.dto.ServletDTO">
		SELECT sgg_nm, sgg_cd 
		FROM tl_sgg
		WHERE sd_nm=#{text}
		
	</select>
	<select id="suryeon" parameterType="String" resultType="servlet.dto.ServletDTO">
		SELECT bjd_nm, bjd_cd 
		FROM tl_bjd
		
	</select>
</mapper>